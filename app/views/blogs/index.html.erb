<p id="notice"><%= notice %></p>

<h1>Blogs</h1>
<%= @blogs.size %>

<%= turbo_frame_tag 'blog-list' do %>
  <%= form_with url: blogs_path, method: :get do |f| %>
    <%= f.label :search_title, 'タイトル検索' %>
    <%= f.text_field :search_title %>
    <%= f.submit %>
  <% end %>

  <%= render 'list', blogs: @blogs %>
<% end %>

<br>

<%= link_to 'New Blog', new_blog_path %>
